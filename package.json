    "cookie": "^0.4.2",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "framer-motion": "^6.3.4",
    "google-fonts-helper": "^3.1.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "next": "^13.1.2",
    "node-mocks-http": "^1.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-swipeable": "^6.2.1",
    "swell-node": "^4.0.10",
    "swiper": "^8.4.5",
    "tailwindcss-radix": "^2.6.0",
    "undici": "^5.13.0",
    "use-debounce": "^8.0.1",
    "zustand": "^3.7.1"
